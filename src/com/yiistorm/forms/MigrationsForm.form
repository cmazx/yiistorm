<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.yiistorm.forms.MigrationsForm">
  <grid id="27dc6" binding="contentPane" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="569" height="254"/>
    </constraints>
    <properties>
      <enabled value="true"/>
    </properties>
    <border type="none"/>
    <children>
      <scrollpane id="b40bc" class="com.intellij.ui.components.JBScrollPane" binding="scrollpane">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="6b831" class="javax.swing.JTextArea" binding="migrateLog">
            <constraints/>
            <properties>
              <columns value="0"/>
              <editable value="false"/>
              <enabled value="true"/>
              <lineWrap value="true"/>
              <text value="Welcome to YiiStorm migrations!"/>
            </properties>
          </component>
        </children>
      </scrollpane>
      <grid id="f662a" layout-manager="GridLayoutManager" row-count="1" column-count="4" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="2aa07" class="javax.swing.JButton" binding="createMigration">
            <constraints>
              <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="4" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Create migration"/>
            </properties>
          </component>
          <component id="9b52b" class="javax.swing.JButton" binding="applyMigration">
            <constraints>
              <grid row="0" column="3" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Apply all  migrations"/>
            </properties>
          </component>
          <component id="cf4c0" class="javax.swing.JLabel">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="New migration:"/>
            </properties>
          </component>
          <component id="c092b" class="javax.swing.JTextField" binding="createMigrationName">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
            </constraints>
            <properties>
              <toolTipText value="New migration label"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
